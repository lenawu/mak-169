		<header>
			<div class="container">
				<div class="row">
				  <%= link_to root_path do %>
					  <div class="responsive-logo"></div>
					<% end %>
					<div class="pullcontainer">
						<a href="#" id="pull"><i class="fa fa-bars fa-2x"></i></a>
					</div>
				</div>
				<div class="row">
					<div class="col-md-8 col-md-offset-2">
						<nav>
						 <!-- <%= link_to root_path do %>-->
						 <a href="/">
						 	
					      <div class="logo-holder"></div>
						 </a>
				    	<% end %>
							<ul class="clearfix">
							  <% if user_signed_in? %>
                  				<li><%= link_to "Log Out", destroy_user_session_path, method: :delete, class: "page-link" %></li>
                  				<li><%= link_to "Deadlines", deadlines_path, class: "page-link"%></li>
